Page{
  width: 100%;
  height: 100%
}
.whole{
  width: 100%;
  height: 100%;
  position: relative

}
.whole>image{
width: 100%;
height: 100%;

position: absolute;
top: 0;
left: 0;
}
.com{
  width: 84%;
  height: 100%;
  margin: 0 auto;
  padding-top: 120rpx;
  box-sizing: border-box
}
.com>image{
  width: 420rpx;
  height: 230rpx;
  margin: 0 auto;
  display: block;
  z-index: 999;
  position: relative;
  margin-bottom: 138rpx;
}
.com form{
  width: 100%;
  height: 450rpx;
  z-index: 999;
    position: relative
}
.com .phone{
  width: 100%;
  height: 100rpx;
  position: relative
}
.com .phone> text{
  display: block;
  font-size:24rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(200,200,200,1);
}
.com .phone>input{
  line-height: 60rpx;
  width: 100%;
  height: 60rpx;
  border-bottom: 2rpx solid rgba(255,255,255,0.3);
  font-size:32rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(200,200,200,1);
}
.com .phone>input[placeholder]{
    line-height: 60rpx;
  font-size:32rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(200,200,200,0.2);
}
.com .phone>.ma{
position: absolute;
right: 0;
font-size:24rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(61,178,163,1);
bottom: 19rpx;
z-index: 9999999999999999999999999999
}
.com .yanzheng{
  width: 100%;
  height: 100rpx;
  position: relative;
  margin-top: 40rpx;
}
.com .yanzheng>text{
  display: block;
  font-size:24rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(200,200,200,1);
}
.com .yanzheng>input{
  line-height: 60rpx;
  width: 100%;
  height: 60rpx;
  border-bottom: 2rpx solid rgba(255,255,255,0.3);
  font-size:32rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(200,200,200,1);
}
.com .yanzheng>input[placeholder]{
    line-height: 60rpx;
  font-size:32rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(200,200,200,0.2);
}
.com .yue{
  margin-top: 30rpx;
 margin-bottom: 170rpx;
}
.com .yue>checkbox{
  width: 24rpx!important;
  height: 24rpx!important;
  float: left;
}
checkbox .wx-checkbox-input {
  width: 24rpx;
  height: 24rpx;
}
.com .yue>text{
  float: left;
  margin-left: 26rpx;
  font-size:24rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(150,150,150,1);
margin-top: 10rpx;
}
.com>form button{
  width:100%;
height:88rpx;
background:rgba(61,178,163,1);
border-radius:44rpx;
font-size:34rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(255,255,255,1);
text-align: center;
line-height: 88rpx;
margin-top: 88rpx;

}
