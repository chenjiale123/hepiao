font-weight:bold;
color:rgba(41,48,69,1);
margin-bottom: 30rpx;
}
.mainIn>button{
position: absolute;
bottom: 0rpx;
right: 0;
width:128rpx;
height:58rpx;
border:2px solid rgba(61,178,163,1);
border-radius:29rpx;
font-size:26rpx;
font-family:PingFang-SC-Bold;
font-weight:bold;
color:rgba(61,178,163,1);
text-align: center;
line-height: 58rpx;
}
.com>button{
  width: 180rpx;
  height: 80rpx;
  display: block;
  margin: 0 auto;
  margin-top: 99rpx;
  border-radius: 40rpx;
  position: relative


}
.com>button>image{
  width: 180rpx;
  height: 80rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.com>button>text{
  z-index: 999;
  position: relative;
  display: block;
  font-size:36rpx;
font-family:PingFang-SC-Bold;
font-weight:bold;
color:rgba(255,255,255,1);
line-height: 80rpx;
}
